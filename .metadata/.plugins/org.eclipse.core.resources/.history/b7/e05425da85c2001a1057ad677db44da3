	<%
		String userID = request.getParameter("userID");
		String userPW = request.getParameter("userPW");
		
		dbConnection objDB = new dbConnection();
	
		try{
		objDB.select("SELECT COUNT(*) FROM admTbl WHERE userID = '" + userID + "' and userPW = " + userPW + "'" );
			
